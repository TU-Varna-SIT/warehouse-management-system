            <TableColumn fx:id="cityColumn" maxWidth="128.0" minWidth="128.0" prefWidth="128.0" text="City" />
            <TableColumn fx:id="countryColumn" maxWidth="128.0" minWidth="128.0" prefWidth="128.0" text="Country" />
