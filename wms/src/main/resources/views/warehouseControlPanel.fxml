<AnchorPane prefHeight="480.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bg.tuvarna.sit.wms.controllers.WarehouseControlPanelController">
    <TableView fx:id="warehousesTable" layoutX="79.0" layoutY="90.0" prefHeight="274.0" prefWidth="648.0">
